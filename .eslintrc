{
  "extends": "airbnb-base",
  "rules": {
    "max-len": ["error", 120, 4, {
        "ignoreComments": true,
        "ignoreUrls": true,
        "ignorePattern": "^\\s*var\\s.+=\\s*(require\\s*\\()|(/)"
    }],
    "prefer-destructuring": ["off"],
    "no-param-reassign": ["off"],
    "func-names": ["off"],
    "consistent-return": ["off"],
    "no-console": ["error"]
  },
  "parserOptions": {
    "ecmaVersion": 2017
  },
  "globals": {
    "Customer": true,
    "Contact": true,
    "FileStorage": true,
    "FileContainer": true,
    "ContactView": true,
  },
}
